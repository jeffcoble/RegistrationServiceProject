<?xml version="1.0" encoding="UTF-8"?>

<ejb-jar xmlns = "http://java.sun.com/xml/ns/javaee" 
         version = "3.1"
         xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation = "http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">

<enterprise-beans>
    <session>
      <ejb-name>RegistrationServiceBeanImpl</ejb-name>
      <ejb-class>org.engineeringnotebook.registrationservice.ejb.RegistrationServiceBeanImpl</ejb-class>

      <env-entry>
        <env-entry-name>org/engineeringnotebook/amazonaccesskey</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>YOUR_KEY</env-entry-value>
      </env-entry>

      <env-entry>
        <env-entry-name>org/engineeringnotebook/amazonsecretkey</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>YOUR_KEY</env-entry-value>
      </env-entry>

      <env-entry>
        <env-entry-name>org/engineeringnotebook/twitterconsumerkey</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>YOUR_KEY</env-entry-value>
      </env-entry>
   
      <env-entry>
        <env-entry-name>org/engineeringnotebook/twitterconsumersecretkey</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>YOUR_KEY</env-entry-value>
      </env-entry> 
    </session>
  </enterprise-beans>
  
</ejb-jar>
